<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" width="512" height="512">
  <defs>
    <style>
      .icon-bg { fill: #0b0f14; }
      .icon-primary { fill: #66e0ff; }
      .icon-secondary { fill: #8aff80; }
      .icon-surface { fill: #0f141b; }
    </style>
    
    <!-- Enhanced gradients for high resolution -->
    <radialGradient id="bgGradient" cx="25%" cy="25%">
      <stop offset="0%" style="stop-color:#66e0ff;stop-opacity:0.15"/>
      <stop offset="50%" style="stop-color:#8aff80;stop-opacity:0.08"/>
      <stop offset="100%" style="stop-color:#0b0f14;stop-opacity:1"/>
    </radialGradient>
    
    <radialGradient id="centerGradient" cx="50%" cy="50%">
      <stop offset="0%" style="stop-color:#8aff80;stop-opacity:1"/>
      <stop offset="40%" style="stop-color:#66e0ff;stop-opacity:0.9"/>
      <stop offset="100%" style="stop-color:#0b0f14;stop-opacity:0.3"/>
    </radialGradient>
    
    <!-- Subtle shadow effect -->
    <filter id="dropShadow">
      <feDropShadow dx="0" dy="4" stdDeviation="8" flood-color="#000" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Background with iOS-style rounded corners -->
  <rect width="512" height="512" rx="64" ry="64" fill="url(#bgGradient)"/>
  
  <!-- Main hash grid structure -->
  <g class="icon-primary" opacity="0.7" filter="url(#dropShadow)">
    <!-- Vertical lines -->
    <rect x="128" y="80" width="20" height="352" rx="10"/>
    <rect x="364" y="80" width="20" height="352" rx="10"/>
    
    <!-- Horizontal lines -->
    <rect x="80" y="128" width="352" height="20" rx="10"/>
    <rect x="80" y="364" width="352" height="20" rx="10"/>
  </g>
  
  <!-- Hash distribution nodes -->
  <g opacity="0.6">
    <circle cx="148" cy="148" r="16" class="icon-primary"/>
    <circle cx="364" cy="148" r="16" class="icon-primary"/>
    <circle cx="148" cy="364" r="16" class="icon-primary"/>
    <circle cx="364" cy="364" r="16" class="icon-primary"/>
  </g>
  
  <!-- Central jump hash algorithm representation -->
  <g filter="url(#dropShadow)">
    <circle cx="256" cy="256" r="64" fill="url(#centerGradient)"/>
    <circle cx="256" cy="256" r="32" class="icon-bg" opacity="0.8"/>
    <circle cx="256" cy="256" r="16" class="icon-secondary"/>
    
    <!-- Inner algorithm symbol -->
    <g class="icon-bg" opacity="0.9">
      <!-- Hash symbol representation -->
      <rect x="244" y="240" width="4" height="32" rx="2"/>
      <rect x="264" y="240" width="4" height="32" rx="2"/>
      <rect x="240" y="248" width="32" height="4" rx="2"/>
      <rect x="240" y="260" width="32" height="4" rx="2"/>
    </g>
  </g>
  
  <!-- Subtle connection lines showing hash distribution -->
  <g class="icon-secondary" opacity="0.3" stroke-width="2" fill="none" stroke="#8aff80">
    <line x1="148" y1="148" x2="220" y2="220"/>
    <line x1="364" y1="148" x2="292" y2="220"/>
    <line x1="148" y1="364" x2="220" y2="292"/>
    <line x1="364" y1="364" x2="292" y2="292"/>
  </g>
</svg>
